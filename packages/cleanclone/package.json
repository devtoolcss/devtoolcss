{
  "name": "cleanclone",
  "version": "0.0.8",
  "description": "Inline DevTool-parsed CSS for any website and crawl it on the fly.",
  "license": "MIT",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:devtoolcss/devtoolcss.git"
  },
  "homepage": "https://github.com/devtoolcss/devtoolcss#readme",
  "keywords": [
    "css",
    "html",
    "web-crawler",
    "inline",
    "devtools-protocol"
  ],
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "dependencies": {
    "@devtoolcss/inliner": "workspace:*",
    "@devtoolcss/inspector": "workspace:*",
    "chalk": "^5.6.2",
    "chrome-launcher": "^1.2.1",
    "chrome-remote-interface": "^0.33.3",
    "escape-html": "^1.0.3",
    "escape-string-regexp": "^5.0.0",
    "ink": "^6.3.0",
    "js-beautify": "^1.15.4",
    "jsdom": "^26.1.0",
    "react": "^19.1.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/js-beautify": "^1.14.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.13",
    "@types/yargs": "^17.0.33",
    "devtools-protocol": "0.0.927104"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "pnpm build && chmod +x ./dist/cli.js",
    "type-check": "tsc --noEmit"
  },
  "bin": {
    "cleanclone": "dist/cli.js"
  }
}
