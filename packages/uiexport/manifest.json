{
  "manifest_version": 3,
  "name": "UI Export",
  "version": "1.0.1",
  "description": "Export any components with CSS inlined.",
  "permissions": ["debugger"],
  "devtools_page": "devtools.html",
  "icons": {
    "128": "icons/128.png"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}
